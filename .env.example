# Firebase Configuration
VITE_FIREBASE_API_KEY=your_firebase_api_key
VITE_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
VITE_FIREBASE_PROJECT_ID=your-project-id
VITE_FIREBASE_STORAGE_BUCKET=your-project.firebasestorage.app
VITE_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
VITE_FIREBASE_APP_ID=your_app_id

# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Anthropic Claude AI Configuration
VITE_ANTHROPIC_API_KEY=sk-ant-api03-your_anthropic_key
ANTHROPIC_API_KEY=sk-ant-api03-your_anthropic_key

# Claude AI Settings
# Recomendado fixar snapshot para produção (consistência)
VITE_ANTHROPIC_MODEL=claude-sonnet-4-5-20250929
ANTHROPIC_MODEL=claude-sonnet-4-5-20250929

# Development Mode
# 1 = usar dados mock (para testes sem consumir API)
# 0 = usar API real
VITE_AI_REPORT_USE_MOCK=0
AI_REPORT_USE_MOCK=0

# Microsoft Fabric to Supabase Pipeline
# Configure estas variáveis para o pipeline fabric_to_supabase.py
SUPABASE_URL=https://seu-projeto.supabase.co
SUPABASE_KEY=sua-chave-service-role-aqui
